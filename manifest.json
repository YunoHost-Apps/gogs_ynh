{
    "name": "Gogs",
    "id": "gogs",
    "packaging_format": 1,
    "description": {
        "en": "Lightweight Git forge",
        "fr": "Forge Git légère"
    },
    "version": "0.12.3~ynh1",
    "url": "http://gogs.io",
    "license": "MIT",
    "maintainer": {
        "name": "",
        "email": ""
    },
    "previous_maintainers": {
        "name": "tostaki",
        "email": "maxime@max.privy.place"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "mysql"
    ],
    "requirements": {
        "yunohost": ">= 4.2.4"
    },
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/gogs",
                "default": "/gogs"
            },
            {
                "name": "admin",
                "type": "user"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
